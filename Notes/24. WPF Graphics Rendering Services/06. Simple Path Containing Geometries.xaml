<Page
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Canvas Width="640" Height="480" Background="LightSteelBlue">
    <Path Fill="Orange" Stroke="Blue" StrokeThickness="5">
      <Path.Data>
        <GeometryGroup>
          <EllipseGeometry Center="75,70" RadiusX="30" RadiusY="30"></EllipseGeometry>
          <RectangleGeometry Rect="25,55 100 30"></RectangleGeometry>
          <LineGeometry StartPoint="0,0" EndPoint="70,30"></LineGeometry>
          <LineGeometry StartPoint="70,30" EndPoint="0,30"></LineGeometry>
        </GeometryGroup>
      </Path.Data>
    </Path>
  </Canvas>
</Page>